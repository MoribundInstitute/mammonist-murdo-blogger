<script type="text/javascript">
//<![CDATA[
function getPopularPosts(json) {
  var popularPosts = '';
  for (var i = 0; i < json.feed.entry.length && i < 4; i++) {
    var entry = json.feed.entry[i];
    var postTitle = entry.title.$t;
    var postUrl;
    for (var k = 0; k < entry.link.length; k++) {
      if (entry.link[k].rel == 'alternate') {
        postUrl = entry.link[k].href;
        break;
      }
    }
    var postThumbnail = "https://lh3.googleusercontent.com/-zFdxYk-oMRQ/VVVYFhKl4BI/AAAAAAAACVs/DAaMCXNNpwg/w72-h72-p-k-no-nu/nothumb.png";
    if ("media$thumbnail" in entry) {
      postThumbnail = entry.media$thumbnail.url;
    }
    
    popularPosts += '<li>';
    popularPosts += '<div class="item-thumbnail"><a href="' + postUrl + '"><img alt="" src="' + postThumbnail + '"/></a></div>';
    popularPosts += '<div class="item-title"><a href="' + postUrl + '">' + postTitle + '</a></div>';
    popularPosts += '</li>';
  }
  document.getElementById('popular-posts').innerHTML = '<h2>Popular Posts</h2><ul>' + popularPosts + '</ul>';
}
//]]>
</script>
<script type="text/javascript" src="https://www.yourblog.blogspot.com/feeds/posts/default?orderby=popularity&alt=json-in-script&max-results=4&callback=getPopularPosts"></script>


